<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>users</title>
</head>
<body>
<table cellpadding="5" border="1px">
    <thead>
    <tr>
        <%Object.keys(users[0]).forEach(function (item) {%>
              <td><%=item%></td>
        <%})%>
    </tr>
    </thead>
    <tbody>
    <%users.forEach(function (user) {%>
        <tr>
            <td><%=user.name%></td>
            <td><%=user.date%></td>
            <td><%=user.age%></td>
        </tr>
    <%})%>
    </tbody>
</table>
</body>
</html>